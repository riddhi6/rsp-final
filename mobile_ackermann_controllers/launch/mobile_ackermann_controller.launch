<launch>
	<rosparam file ="$(find mobile_ackermann_controllers)/controllers/ackermann_controller.yaml" command="load" />
	<rosparam file ="$(find mobile_ackermann_controllers)/controllers/joint_state_controller.yaml" command="load"/>

	<node name="mobile_ackermann_controller"
			pkg="controller_manager"
			type="controller_manager"
			args="spawn mobile_ackermann_controller"
			respawn="false"
			output="screen"/>

	<node name="joint_state_controller"
			pkg="controller_manager"
			type="controller_manager"
			args="spawn joint_state_controller"
			respawn="false"
			output="screen"/>
</launch>